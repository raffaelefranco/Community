<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="it.unisannio.studenti.franco.raffaele.communityandroidclient">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/people"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AllOpenedRequestActivity"></activity>
        <activity android:name=".AnswerActivity"></activity>
        <activity android:name=".ClosedRequestActivity"></activity>
        <activity android:name=".CloseRequestActivity"></activity>
        <activity android:name=".InfoActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".MenuActivity"></activity>
        <activity android:name=".NewRequestActivity"></activity>
        <activity android:name=".OpenedRequestActivity"></activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".ViewClosedRequestActivity"></activity>
        <activity android:name=".ViewOpenedRequestActivity"></activity>
        <activity android:name=".ViewResponseActivity"></activity>


    </application>

</manifest>